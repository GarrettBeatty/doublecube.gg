{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DynamoDb": {
    "TableName": "backgammon-local"
  },
  "Analysis": {
    "EvaluatorType": "Gnubg"
  },
  "Gnubg": {
    "ServiceUrl": "http://gnubg-service",
    "EvaluationPlies": 2,
    "VerboseLogging": true
  },
  "Puzzle": {
    "EvaluatorType": "Gnubg",
    "EnableAutomaticGeneration": true
  }
}
