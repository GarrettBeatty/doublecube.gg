{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Information"
    }
  },
  "AllowedHosts": "*",
  "DynamoDb": {
    "TableName": "backgammon-prod"
  },
  "AWS": {
    "Region": "us-east-1"
  },
  "Redis": {
    "ConnectionString": ""
  },
  "Jwt": {
    "Secret": "LOCAL-DEV-ONLY-DO-NOT-USE-IN-PRODUCTION-CHANGE-ME-32CHARS!",
    "Issuer": "BackgammonServer",
    "Audience": "BackgammonClient",
    "ExpirationDays": "7"
  },
  "Features": {
    "BotGamesEnabled": false,
    "MaxBotGames": 1,
    "BotGameRestartDelaySeconds": 12
  },
  "CacheSettings": {
    "UserProfile": {
      "Expiration": "00:05:00",
      "LocalCacheExpiration": "00:01:00"
    },
    "PlayerStats": {
      "Expiration": "00:15:00",
      "LocalCacheExpiration": "00:03:00"
    },
    "PlayerGames": {
      "Expiration": "00:10:00",
      "LocalCacheExpiration": "00:02:00"
    },
    "Friends": {
      "Expiration": "00:05:00",
      "LocalCacheExpiration": "00:01:00"
    }
  },
  "Analysis": {
    "EvaluatorType": "Gnubg",
    "AllowUserSelection": true
  },
  "Gnubg": {
    "ExecutablePath": "gnubg",
    "TimeoutMs": 30000,
    "VerboseLogging": true,
    "EvaluationPlies": 2,
    "UseNeuralNet": true
  },
  "EloRating": {
    "StartingRating": 1500,
    "KFactorNew": 32,
    "KFactorEstablished": 24,
    "GamesForEstablished": 30
  }
}
